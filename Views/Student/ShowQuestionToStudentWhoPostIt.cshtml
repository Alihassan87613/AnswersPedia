                                $('#showC2').append(user);
                                $('#commaQuest').val('');
                            }
                            else
                            {
                                var date = new Date(Date.parse(item.Date_Comment));

                                var user = '<span class="Emal">' + item.T_UserName + '</span><br><span class="Questi">' + item.Comment + '</span><br><span class="Time"></span><span class="Time">' + date + '</span><br/>';
                                $('#showC2').append(user);
                                $('#commaQuest').val('');
                            }
                        });
                    },
                    error: function (err) {
                        alert(err);
                    }
                });
            }
        }
</script>                             
    } 
